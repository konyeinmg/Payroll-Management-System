<Window x:Class="Payroll_Management_System.Kickout"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        Icon="D:\Nyein Chan Aung\C# Problems\Photos\kickout.png"
        xmlns:local="clr-namespace:Payroll_Management_System"
        mc:Ignorable="d"
        Title="Kickout" Height="130" Width="300" ResizeMode="CanMinimize" WindowStartupLocation="CenterScreen">
    <Grid>
        <StackPanel Margin="2,2,-0.333,2" VerticalAlignment="Top" Height="30">
            <TextBlock Foreground="red" VerticalAlignment="Center"  Margin="96,5,0,5" HorizontalAlignment="Left" Width="90" Text="Kick Out" FontSize="14.667" FontWeight="Bold" ></TextBlock>
        </StackPanel>
        <RadioButton x:Name="permanent" Content="Permanent Staff" HorizontalAlignment="Left" Margin="25,58,0,0" VerticalAlignment="Top" Foreground="DarkBlue" Checked="permanent_check"/>
        <RadioButton x:Name="temporary" Content="Temporary Staff" HorizontalAlignment="Left" Margin="163,59,0,0" VerticalAlignment="Top" Foreground="DarkBlue" Checked="temporary_check" />
    </Grid>
</Window>
